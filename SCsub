Import('env')
Import('env_modules')

env_modules.add_source_files(env.modules_sources,"src/v3/spine/*.c")
env_modules.add_source_files(env.modules_sources,"src/v4/spine/*.c")

env_modules.add_source_files(env.modules_sources,"*.cpp")

env_modules.Append(CPPFLAGS=[
	"-I", "modules/spine/include",
])

Export('env_modules')
Export('env')


